{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "tsConfigPath": "tsconfig.build.json",
    "deleteOutDir": true,
    "assets": [
      {
        "include": "../generated/**/*",
        "outDir": "dist/apps/server/generated"
      }
    ]
  },
  "generateOptions": {
    "spec": false
  },
  "entryFile": "apps/server/src/main"
}
